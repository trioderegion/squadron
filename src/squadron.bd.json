{
  "name": "squadron",
  "title": "Squadron",
  "description": "Quickly create marching orders or formations for your tokens. Easy jump in and out of your formation or halt others from following you.",
  "version": "dev",
  "entryPoints": {
    "main": "squadron.mjs",
    "lang": "lang/*"
  },
  "profile": {
    "dev": {
      "dest": "../dev"
    },
    "release": {
      "dest": "../dist",
      "clean": "true"
    }
  },
  "dependencies": {
    "core": [
      "11",
      "11.313"
    ],
    "modules": {
      "warpgate": [
        "1.19.0"
      ]
    }
  },
  "authors": [
    {
      "name": "badgerwerks",
      "url": "https://github.com/trioderegion"
    }
  ],
  "relationships": {
    "requires": [{
      "id": "warpgate",
      "type": "module",
      "manifest": "https://github.com/trioderegion/warpgate/releases/latest/download/module.json",
      "compatibility": {
        "minimum": "1.12.1",
        "verified": "1.15.x",
        "maximum": "2"
      }
    }]
  },
  "projectUrl": "https://github.com/trioderegion/squadron"
}
