{
  "title": "Squadron",
  "description": "Quickly create marching orders or formations for your tokens. Easy jump in and out of your formation or halt others from following you.",
  "version": "dev",
  "entryPoints": {
    "main": "squadron.mjs",
    "lang": "lang/*"
  },
  "static": "*.hbs",
  "profile": {
    "dev": {
      "dest": "../dev",
      "clean": false,
      "hmr":true,
      "compress": true,
      "sourcemaps":true
    },
    "release": {
      "dest": "../dist",
      "clean": true
    }
  },
  "dependencies": {
    "core": [
      "11"
    ],
    "modules": {
      "warpgate": [
        "1.19.0"
      ]
    }
  },
  "authors": [
    {
      "name": "badgerwerks",
      "url": "https://github.com/trioderegion"
    }
  ],
  "projectUrl": "https://github.com/trioderegion/squadron"
}
